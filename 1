  <script>
    $(document).ready(function() {

        $('#policy_acepted').next('div').css('transition','all 0.3s ease-in').css('white-space','pre');
        $('input.btn.confirm.btn-primary').on("click",function(event) {

            if($('.CC').val() != ''){
                event.preventDefault();
                event.stopPropagation();
                return false;
            }

            if($('#policy_acepted').is(':checked') == false){

                $('html, body').animate({
                    scrollTop: $("#policy_acepted").offset().top
                }, 200);
                $('#policy_acepted').next('div').css('transition','all 0.3s ease-in').css('white-space','pre').css('color','red').delay(2050).queue(function() {
                    $('#policy_acepted').next('div').css('color','black')
                    $(this).dequeue();
                });
                event.preventDefault();
                event.stopPropagation();
            } else if($('#storno_acepted').length > 0 && $('#storno_acepted').is(':checked') == false) {
                $('html, body').animate({
                    scrollTop: $("#policy_acepted").offset().top
                }, 200);
                $('#storno_acepted').next('div').css('transition','all 0.3s ease-in').css('white-space','pre').css('color','red').delay(2050).queue(function() {
                    $('#storno_acepted').next('p').css('color','black')
                    $(this).dequeue();
                });

                event.preventDefault();
                event.stopPropagation();
            }
        });
    });
    </script>
