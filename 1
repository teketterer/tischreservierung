  <body><header class="tmpl-header"><div class="header-bar"></div><div class="container"></div></header><div class="jumbotron"></div><main class="tmpl-content  container"><div id="content"><div class="HP-section"><div class="HP-section-title">Tischreservierung Seehotel Louise</div><div xmlns="http://www.w3.org/1999/xhtml" xmlns:file="swo.xml.file">
  <script type="text/javascript">
$(document).ready(function() {

    $('#date').addClass('notranslate');
  $('.available').on("click",function() {
    
    $('button.next').removeAttr('disbabled').addClass('btn-primary');
    $('input[name=time]').val($(this).data('time'));
    $('.timeslot').removeClass('active');
    $(this).addClass('active');
    
    
  });
  
  $('.dropdown-menu a').on("click",function() {
    
    $('.widget-persons').text($(this).text());
    $('input[name=persons]').val($(this).data('num'));
    $(this).closest('.num').find('.dropdown-toggle').text($(this).data('num') + ' Pers.');
    
    loadTimeSlots();
    
  });
  
  $('button.next').on("click",function() {
    
    if(!$(this).attr('disbabled')) {
      
      $('.widget-persons').text($('input[name="persons"]').val() + ' Personen');
      $('.widget-date').text($('.date-value').text() + ' ' + $('input[name="time"]').val()  + ' Uhr');
      
      $('.booking-details').hide();
      $('.contact-information').show();
      
    }
    
    
  });
  
  
  $('.back').on("click",function() {
  
    $('.contact-information').hide();
    $('.booking-complete').hide();
    $('.booking-details').show();
    
    loadTimeSlots();
    
  });
  
  
  $('.dropdown-toggle').on("change",function() {
    loadTimeSlots();
  });
  
   $('.dropdown-toggle').ready(function() {
    loadTimeSlots();
  });
  

  var loadTimeSlots = function() {
    
    var dateText = $('input[name="date"]').val();
    var personsText = $('input[name="persons"]').val();
    if($('html').attr('lang') != 'de'){
        dateText = moment(dateText, 'DD.MM.YYYY', true).format('YYYY-MM-DD');
    }
    $.get(window.location.pathname, {date: dateText, persons: personsText}, function(data) {

        var data = jQuery.parseJSON(data);
        var sumPersons = parseInt(data.sumPersons);
        if('' == '1'){
            sumPersons = 8;
        }

        var opening = data.opening;
        if(opening.is_open == 1) {

            $('.closed_description').slideUp(133);
            $('.timeslots').slideDown(333);
            $('button.btn.next').show();

        } else{
            $('.timeslots').slideUp(133);
            $('.closed_description').slideDown(333);
            $('button.btn.next').hide();
        }
        var locked = data.locked;

        var data = data.timeslots;

        $('.timeslot').remove();
        $('.timeslots .alert').remove();
        var html = '';
        $('.dropdown-menu a').off('click');
        for(var i = 0; i < sumPersons;i++) {
            var cnt = i+1;
            html = html + '<li> <a data-num="'+cnt+'">'+cnt+' Person</a> </li>';
        }
        $('.persons-listing').html(html);
        $('.dropdown-menu a').on("click",function() {

            $('.widget-persons').text($(this).text());
            $('input[name=persons]').val($(this).data('num'));
            $(this).closest('.num').find('.dropdown-toggle').text($(this).data('num') + ' Pers.');

            loadTimeSlots();

        });



        $.each(data, function(key, item) {
        
        var _item = $('<a  class="timeslot list-group-item" data-time="' + item.value + '">' + item.value + ' </a>');
        
        if(item.enabled) {
          
          _item.addClass('available');
          _item.on("click",function() {
             $('.timeslot').removeClass('active');
             $('button.next').removeAttr('disbabled').addClass('btn-primary');
             $('input[name=time]').val($(this).data('time'));
             $(this).addClass('active');
	   });
	   
	} else {
	  
	  _item.addClass('disabled');
	  // reset selected time
	  if($('input[name=time]').val() == item.value) {
	    $('input[name=time]').val('');
	    $('button.next').attr('disbabled', 'disbabled').removeClass('btn-primary');
	  }
	  
	}
	
	_item.append('<span>' + item.label + '</span>');
	
        $('.timeslots').append(_item);
        
      });
      
      // closed

      if(data.length == 0) {
          if(locked.description != undefined) {

              $('.timeslots').html($('<div class="alert alert-info" role="alert">'+locked.description+'</div>'));

          } else{
              $('.timeslots').html($('<div class="alert alert-info" role="alert"></div>'));
          }

        $('input[name=time]').val('');
	$('button.next').attr('disbabled', 'disbabled').removeClass('btn-primary');
      }

      if($('input[name=time]').val())
        $('a[data-time="' + $('input[name=time]').val() + '"]').addClass('active');
      
    });
    
  };
  
  
  $('#form-reservation').on('submit',function(event) {
    
    event.preventDefault(); // Prevent the form from submitting via the browser
    var form = $(this);
    
    form.append($('<input type="hidden" name="token" value="1" />'));

    // Fallback, beim Absenden wird das Datum auf Format geprüft und in ISO gesendet oder wenn leer
    var newDate = $('#date').val();
    if(newDate == ''){
        newDate = moment(new Date()).format('YYYY-MM-DD');
    }
    if(moment(newDate, 'DD.MM.YYYY', true).isValid()){
        newDate = moment(newDate, 'DD.MM.YYYY', true).format('YYYY-MM-DD');
    }

    $('input[name="date"]').val(newDate);
    var data = $('#form-reservation').serialize();
    
    $.ajax({
      type: form.attr('method'),
      url: form.attr('action'),
      data: data
    }).done(function(data) {

      var data = jQuery.parseJSON(data);
      
      if(data.message) {
        
        $('.booking-complete').show();
        
        $('.booking-complete .alert').removeClass('alert-danger').removeClass('alert-success').addClass('alert-' + data.message_type);
        $('.booking-complete .alert').text(data.message);


       $('.booking-complete').append(data.html);
        
       $('.contact-information').hide();
	   $('.booking-details').hide();
        
      }
      
    }).fail(function(data) {
      // Optionally alert the user of an error here...
        console.log('failed');
    });
    
    
  });
  
  // solte bereits im Header gesetzt werden
    $.datepicker.setDefaults({'dateFormat':'dd.mm.yy'});
  
  $('.btn-date-picker').on("click", function(e) {
    
    $('.timeslots').hide();
    $('#date').show();
    
    var dateToday = new Date();
    
    $('#date').datepicker({
       
       minDate: dateToday,
    dateFormat:'dd.mm.yy',
       onSelect: function(dateText, inst) {
       
         $('.date-value').text(dateText);
         $('input[name="date"]').val(dateText);
         
         loadTimeSlots();
         
         $('#date').hide();
         $('.timeslots').show();
         
       }
       
     }
    );
    $('.ui-datepicker').addClass('notranslate');
    
  });
        // Init einen Trigger für die passende Formatierung
    $('.btn-date-picker').trigger('click'); $('.ui-datepicker-today').trigger('click');
});
</script>
